<h1>{{title}}</h1>
{{#if orders.length}}
  {{#each orders}}
    <div class="row">
        <div class="col s6 offset-s3">
            <div class="card">
                <div class="card-content">
                    <span class="card-title">
                            Order {{_id }}
                    </span>
                    <p class="date">{{date.}}</p>
                    <p><strong>{{user.userId.name}}</strong>({{ user.userId.email }})</p>
                    <ol>
                        {{#each courses}}
                        <li>
                            {{ course.title }}&nbsp; (x&nbsp;<strong>{{count}}</strong>)
                        </li>
                        {{/each}}
                      
                    </ol>
                    <hr/>
                    <p>Price: <span class="price">{{price}}</span></p>
                </div>
            </div>
        </div>
    </div>
  {{/each}}
{{else}}
   <p>Orders cart is empty</p>
{{/if}}